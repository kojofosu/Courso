<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edue.courso">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_grain_image"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_grain_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".About"></activity>
        <activity android:name=".SignIn" />
        <activity android:name=".UploadMaterial" />
        <activity android:name=".Webview" />
        <activity android:name=".StudentMaterials" />
        <activity android:name=".Profile" />
        <activity android:name=".AddMaterials" />
        <activity android:name=".AddNewMaterial" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Home" />
        <activity android:name=".SignUp" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>